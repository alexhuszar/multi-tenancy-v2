      "import": {
        "types": "./dist/theme/index.d.ts",
        "default": "./dist/theme/index.js"
      }
    },
    "./utils": {
      "@multi-tenancy/source": {
        "types": "./src/utils/index.ts",
        "default": "./src/utils/index.ts"
      },
      "import": {
        "types": "./dist/utils/index.d.ts",
        "default": "./dist/utils/index.js"
      }
