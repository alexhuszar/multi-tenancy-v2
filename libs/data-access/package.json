      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "!dist/**/*.spec.*",
    "!dist/**/*.test.*"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0"
  }
