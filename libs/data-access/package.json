{
  "name": "@multi-tenancy/data-access",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "@multi-tenancy/source": {
        "types": "./src/index.ts",
