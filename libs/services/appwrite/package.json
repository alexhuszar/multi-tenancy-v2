{
  "name": "@multi-tenancy/appwrite",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "@multi-tenancy/source": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "!dist/**/*.spec.*",
    "!dist/**/*.test.*"
  ],
  "sideEffects": false,
  "dependencies": {
    "next": "^16.0.1",
    "@swc/helpers": "0.5.18",
    "node-appwrite": "^22.1.2"
  }
}
